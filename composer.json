{
    "name": "nextgenbite/laravel-shipping",
    "description": "Shipping package for Laravel. Supported providers: CanadaPost, USPS, UPS, FedEx and Purolator.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ups",
        "fedex",
        "rest",
        "canadapost",
        "usps",
        "purolator",
        "shipping",
        "rate",
        "rating"
    ],
    "authors": [
        {
            "name": "nextgenbite",
            "email": "iliusrahman@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : ">=8.2",
        "ext-soap": "*",
        "openlss/lib-array2xml": "^1.0",
        "psr/log": "^3.0",
        "illuminate/collections": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/testing": "^11.0",
        "illuminate/support": "^11.0",
        "phpoption/phpoption": "^1.9",
        "vlucas/phpdotenv": "^5.5",
        "illuminate/database": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mitrik\\Shipping\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mitrik\\Shipping\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mitrik\\Shipping\\ShippingServiceProvider"
            ]
        }
    }
}
